   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"vM", "=", "varM"}], ",", " ", "a", ",", "b", ",", "c", ",", 
       "d", ",", "Res"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Let", "'"}], "s", " ", "check", " ", "if", " ", "the", " ", 
       "matrix", " ", "is", " ", "of", " ", "size", " ", "2", "x2"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Dimensions", "[", "vM", "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "2"}], " ", "&&", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"Dimensions", "[", "vM", "]"}], "[", 
           RowBox[{"[", "2", "]"}], "]"}], "\[Equal]", "2"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"a", "=", 
          RowBox[{"vM", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"b", "=", 
          RowBox[{"vM", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"c", "=", 
          RowBox[{"vM", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "1"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"d", "=", 
          RowBox[{"vM", "[", 
           RowBox[{"[", 
            RowBox[{"2", ",", "2"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{
             "The", " ", "matrix", " ", "is", " ", "invertible", " ", "if", 
              " ", "and", " ", "only", " ", "if", " ", "ad"}], "-", "bc"}], 
            "\[NotEqual]", "0"}], " ", "..."}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"a", "*", "d"}], "-", 
             RowBox[{"b", "*", "c"}]}], "\[Equal]", "0"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
           "Print", "[", "\"\<[ERROR] The matrix is not invertible.\>\"", 
            "]"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
          "Use", " ", "the", " ", "provided", " ", "equation", " ", "to", " ",
            "calculate", " ", "the", " ", "inverse", " ", "of", " ", "the", 
           " ", "2", "x2", " ", "matrix"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"Res", "=", 
          RowBox[{
           FractionBox["1", 
