file = {:C$\backslash$:/Users/oskat/Dropbox/PAPER/Papers/Su-8.pdf:pdf},
issn = {00304026},
journal = {Optik},
keywords = {Laser direct writing,Laser processing,Optical waveguide,SU-8},
number = {4},
pages = {1651--1655},
publisher = {Elsevier GmbH.},
title = {{405 nm laser processing of thin SU-8 polymer film}},
url = {http://dx.doi.org/10.1016/j.ijleo.2015.11.057},
volume = {127},
year = {2016}
}
@article{Sharma2010,
